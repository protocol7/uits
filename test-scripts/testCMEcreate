# Test 12	ProductID_completed present. should fail
echo "CME Test 12 ... \c"
`./UITS_Tool cme_create \
--silent \
--xsd $default_xsd \
--cme_uits $default_uits \
--algorithm $default_algorithm \
--priv  $default_priv \
