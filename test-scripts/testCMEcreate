--URL $default_URL\
--PA $default_PA \
--Copyright $default_Copyright \
--Extra $default_Extra \
--Extra_type $default_Extra_type \
--Time $default_Time`
exit_status=$?
if [ $exit_status != 132 ]; then	# should fail with an exit status of 132
 echo "FAIL"
else
 echo "PASS"
fi

# Test 5	priv missing				Should fail
echo "CME Test 5 ... \c"
